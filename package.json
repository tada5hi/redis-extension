{
    "name": "redis-extension",
    "version": "2.0.2",
    "description": "A redis extension.",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "rollup -c",
        "build": "rimraf dist && npm run build:types && npm run build:js",
        "lint": "eslint --ext .js,.ts ./src",
        "lint:fix": "npm run lint -- --fix",
        "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
        "test:coverage": "npm run test -- --coverage",
        "release": "npm run build && changelogen --release --push && npm publish"
    },
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://github.com/tada5hi"
    },
    "license": "MIT",
    "keywords": [
        "redis",
        "store",
        "cache",
        "tracker",
        "scheduler",
        "typescript",
        "cluster",
        "client"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tada5hi/redis-extension.git"
    },
    "bugs": {
        "url": "https://github.com/tada5hi/redis-extension/issues"
    },
    "homepage": "https://github.com/tada5hi/redis-extension#readme",
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^16.0.3",
        "@rollup/plugin-swc": "^0.4.0",
        "@tada5hi/commitlint-config": "^1.2.6",
        "@tada5hi/eslint-config-typescript": "^1.2.17",
        "@tada5hi/tsconfig": "^0.6.0",
        "@types/ioredis-mock": "^8.2.6",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.10.1",
        "cross-env": "^10.1.0",
        "eslint": "^8.37.0",
        "husky": "^9.1.7",
        "ioredis-mock": "^8.13.1",
        "jest": "^30.2.0",
        "rollup": "^4.53.3",
        "testcontainers": "^11.8.1",
        "ts-jest": "^29.4.5",
        "typescript": "^5.9.3",
        "workspaces-publish": "^1.5.0"
    },
    "dependencies": {
        "ioredis": "^5.8.2",
        "smob": "^1.5.0"
    }
}
